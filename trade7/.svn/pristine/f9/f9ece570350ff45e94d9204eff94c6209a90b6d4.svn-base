package com.liantuo.trade.exception;

public class FormatCheckedException extends BusinessException {


	private static final long serialVersionUID = 7732497540467990605L;
	public FormatCheckedException(){
		super();
	}
	public FormatCheckedException(String msg){
		super(msg);
		
	}
	public FormatCheckedException(Throwable cause) {
		super(cause);
	}

	/**
	 * @param message
	 * @param cause
	 */
	public FormatCheckedException(String message, Throwable cause) {
		super(message, cause);
	}
}
