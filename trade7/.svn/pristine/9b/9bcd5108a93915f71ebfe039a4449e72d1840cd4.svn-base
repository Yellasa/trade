    <build>
        <finalName>trade</finalName>
        <plugins>
            <!--Maven编译插件 配置 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.5.1</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
            <!--war包插件 配置 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <version>2.1.1</version>
                <configuration>
                    <webappDirectory>${basedir}/target/www</webappDirectory>
                    <packagingExcludes>WEB-INF/web.xml</packagingExcludes>
                    <!--<warSourceExcludes>*/classes/log4j.properties</warSourceExcludes> -->
                    <webResources>
                        <resource>
                            <!-- 元配置文件的目录，相对于pom.xml文件的路径 -->
                            <directory>src/main/resources</directory>
                            <!-- 是否过滤文件，也就是是否启动auto-config的功能 -->
                            <filtering>true</filtering>
                            <!-- 目标路径 -->
                            <targetPath>WEB-INF/classes</targetPath>
                            <excludes>
                                <exclude>log4j.properties</exclude>
                            </excludes>
                        </resource>
                    </webResources>
                    <warSourceExcludes>log4j.properties</warSourceExcludes>
                </configuration>
            </plugin>
            <!--jar包插件 配置 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>2.4</version>
                <configuration>
                    <finalName>liantuo-ebill-trade-client</finalName>
                    <includes>
                        <include>com/liantuo/trade/client/trade/**</include>
                        <include>com/liantuo/trade/orm/pojo/TradePayment.class</include>
                        <include>com/liantuo/trade/orm/pojo/TradePayment.java</include>
                        <include>com/liantuo/trade/orm/pojo/TradeRefund.class</include>
                        <include>com/liantuo/trade/orm/pojo/TradeRefund.java</include>
                        <include>com/liantuo/trade/orm/pojo/TradeWithdraw.java</include>
                        <include>com/liantuo/trade/orm/pojo/TradeWithdraw.class</include>
                        <include>com/liantuo/trade/orm/pojo/TradeAdjustAccount.java</include>
                        <include>com/liantuo/trade/orm/pojo/TradeAdjustAccount.class</include>
                        <include>com/liantuo/trade/orm/pojo/TradeTransferAccount.java</include>
                        <include>com/liantuo/trade/orm/pojo/TradeTransferAccount.class</include>
                        <include>com/liantuo/trade/orm/pojo/TradeRecharge.java</include>
                        <include>com/liantuo/trade/orm/pojo/TradeRecharge.class</include>
                        <include>com/liantuo/trade/common/**</include>
                        <include>com/liantuo/trade/client/trade/packet/NotifyPacketResultInfo.class</include>
                        <include>com/liantuo/trade/client/trade/packet/NotifyPacketResultInfo.java</include>
                    </includes>
                    <excludes>
                        <exclude>**/pom.xml</exclude>
                    </excludes>
                </configuration>
            </plugin>
            <!-- assembly插件 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.4</version>
            </plugin>
            <!-- Mybatis generator代码生成插件 配置 -->
            <plugin>
                <groupId>org.mybatis.generator</groupId>
                <artifactId>mybatis-generator-maven-plugin</artifactId>
                <version>1.3.2</version>
                <configuration>
                    <!--myBatis-Configuration.cfg.xml位置 -->
                    <configurationFile>src/main/resources/config/mybatis/generatorConfig.xml/</configurationFile>
                    <!--允许移动生成的文件 -->
                    <verbose>true</verbose>
                    <!--允许覆盖生成的文件 -->
                    <overwrite>true</overwrite>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.10</version>
                <configuration>
                    <parallel>methods</parallel>
                    <threadCount>10</threadCount>
                    <skip>true</skip>
                    <!--包含与排除测试用例 -->
                    <!--<includes> <include>**/*Tests.java</include> </includes> <excludes>
                        <exclude>**/*ServiceTest.java</exclude> <exclude>**/TempDaoTest.java</exclude>
                        </excludes> -->
                </configuration>
            </plugin>
        </plugins>
        <!--配置Maven 对resource文件 过滤 -->
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <excludes>
                    <exclude>**/generator.properties</exclude>
                    <exclude>**/generatorConfig.xml</exclude>
                    <exclude>**/log4j.properties</exclude>
                </excludes>
                <filtering>true</filtering>
            </resource>
            <resource>
                <targetPath>${project.build.directory}/classes</targetPath>
                <directory>src/main/java</directory>
                <filtering>true</filtering>
                <includes>
                    <include>**/*.java</include>
                </includes>
            </resource>
            <!--<resource> <directory>src/main/java</directory> <includes> <include>**/*.properties</include>
                <include>**/*.xml</include> </includes> <filtering>true</filtering> </resource> -->
        </resources>
    </build>

